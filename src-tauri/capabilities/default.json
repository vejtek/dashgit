{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**"
        }
      ],
      "deny": []
    },
    "shell:allow-open",
    {
      "identifier": "shell:allow-open",
      "allow": [
        {
          "href": "https://**"
        }
      ]
    }
  ]
}